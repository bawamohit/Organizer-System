            <Button fx:id="logOutButton" alignment="BOTTOM_CENTER" layoutX="8.0" layoutY="36.0" mnemonicParsing="false" onAction="#handleLogOutButtonAction" prefHeight="2.0" prefWidth="150.0" text="Log Out">
